#enable emmc FFU
CONFIG_MMC_FFU=y

CONFIG_BLK_DEV_LOOP=y
# CONFIG_MMC_PERF_PROFILING is not set
# CONFIG_CGROUP_DEBUG is not set
# CONFIG_MSM_SMD_DEBUG is not set
CONFIG_DEBUG_SET_MODULE_RONX=y
# CONFIG_RMNET_DATA_DEBUG_PKT is not set
